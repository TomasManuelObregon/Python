import math

n = input('Ingrese numero entero: ')

if str.isnumeric(n):
    def raiz(x):
        return(x**(1/2))

    x = ((1+raiz(5))/2)
    y = ((1-raiz(5))/2)
    n = int(n)

    def fibonacci(n):
        return ((1/raiz(5))*(x**n-y**n))
    f = math.trunc(fibonacci(n))
    print(f'En la posicion {n} de la sucecion de Fibonacci el numero es {f}')
else:
    print('Lo que ingresaste no es un numero entero')
